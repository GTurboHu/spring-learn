<?xml version="1.0" encoding="UTF-8"?>
<!-- 最基本的命名空间定义和空间装载 -->
<beans
		xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:context="http://www.springframework.org/schema/context"
		xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
http://www.springframework.org/schema/context
https://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="testA" class="com.gth.learnspring.TestA" autowire="byName">
	</bean>
	<bean id="testB" class="com.gth.learnspring.TestB" autowire="byType">
	</bean>
	<bean id="testC" class="com.gth.learnspring.TestC">
	</bean>
	<bean id="testD" class="com.gth.learnspring.TestD">
	</bean>
	<bean id="testF" class="com.gth.learnspring.TestF">
	</bean>
<!--	init-method必须是无参的方法-->
	<bean id="testG" class="com.gth.learnspring.TestG" init-method="init">
	</bean>
</beans>